e":"/var/www/html/currency-vue/src/components/ChartComponent.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"main-container\">\n    <h1 class=\"main-title\">Chart</h1>\n\n    <div class=\"box-wrap\">\n      <div class=\"\">\n        <div class=\"input-div\">\n          <select name=\"\" id=\"\" class=\"custom-input\">\n            <option v-for=\"item in currency_list\" :key=\"item\" value=\"{{ item.ID }}\">{{ item.CharCode }}</option>\n          </select>\n          \n         \n          <span class=\"error-text\" v-show=\"itemExistMsg !== ''\">{{\n            itemExistMsg\n          }}</span>\n        </div>\n        <p v-if=\"errors.length\">\n    <b>Please correct the following error(s):</b>\n    <ul>\n      <li v-for=\"error in errors\" :key=\"error\">{{ error }}</li>\n    </ul>\n  </p>\n      </div>\n    </div>\n    <div v-show=\"noItemSelectedForMoveMsg !== ''\" class=\"error-div\">\n      {{ noItemSelectedForMoveMsg }}\n    </div>\n  </div>\n</template>\n\n<script>\n// var date = new Date();\n  //localStorage.setItem('currencyIsUserLogin','WERTYUIOP')\n// if (localStorage.getItem(\"currencyIsUserLogin\") !== null) {\n//   window.location.href = window.location.origin+'/list';\n// }\nconst APIURL = \"http://localhost/api/\";\nimport axios from \"axios\";\nimport DefaultLayout from \"../layouts/DefaultLayout\";\nimport { validationMixin } from \"vuelidate\";\n\nexport default {\n  name: \"ChartComponent\",\n  mixins: [validationMixin],\n  created() {\n    this.$emit(\"update:layout\", DefaultLayout);\n    this.getCurrencyList()\n  },\n  data() {\n    return {\n      errors: [],\n      currency_list: [],\n      \n    };\n  },\n  methods: {\n    getCurrencyList() {\n      this.currency_list = [\n        {\n          ID: 1,\n          NumCode: 36,\n          CharCode: \"AUD\",\n          Nominal: 1,\n          Name: \"Австралийский доллар\",\n        },\n        {\n          ID: 2,\n          NumCode: 944,\n          CharCode: \"AZN\",\n          Nominal: 1,\n          Name: \"Азербайджанский манат\",\n        },\n        {\n          ID: 3,\n          NumCode: 826,\n          CharCode: \"GBP\",\n          Nominal: 1,\n          Name: \"Фунт стерлингов Соединенного королевства\",\n        },\n      ];\n      /*\n      const data = { token: localStorage.getItem(\"item_management_token\") };\n      const response = axios.get(APIURL + \"api/item/get/list\", {\n        params: data,\n      });\n      response\n        .then((response) => {\n          var currency_list = response.data.data;\n          currency_list.forEach((e) => {\n            this.item_list.push(e.name);\n            this.left_items.push(e.name);\n          });\n        })\n        .catch((e) => {\n          var error = e.response.data;\n          console.log(error);\n        });\n        */\n    },\n    add() {\n      this.errors = [];\n      if (!this.email) {\n        this.errors.push(\"Email required.\");\n      } else if (!ValidateEmail(this.email)) {\n        this.errors.push(\"Email format not correct.\");\n      }\n      if (!this.password) {\n        this.errors.push(\"Password required.\");\n      }\n\n      if (this.errors.length == 0) {\n        const data = {\n          email: this.email,\n          password: this.password,\n        };\n        const response = axios.post(APIURL + \"login\", data);\n        response\n          .then((response) => {\n            console.log(response.data);\n            // localStorage.setItem('user',JSON.stringify(response.data.user))\n            // localStorage.setItem('currencyIsUserLogin',response.data.token)\n          })\n          .catch((e) => {\n            var error = e.response.data;\n            console.log(error);\n          });\n      }\n    },\n  },\n};\n\nfunction ValidateEmail(mail) {\n  if (\n    /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\\.[a-zA-Z0-9-]+)*$/.test(\n      mail\n    )\n  ) {\n    return true;\n  }\n  return false;\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n</style>"]}]}
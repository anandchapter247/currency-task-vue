,UAAA,QAAQ,EAAE,KAAK;AAFJ,SAAb;AAIA,YAAM,QAAO,GAAI,KAAK,CAAC,IAAN,CAAW,MAAK,GAAI,OAApB,EAA6B,IAA7B,CAAjB;AACA,QAAA,QAAO,CACJ,IADH,CACQ,UAAC,QAAD,EAAc;AAClB,UAAA,OAAO,CAAC,GAAR,CAAY,QAAQ,CAAC,IAArB;AACD,SAHH,EAIG,KAJH,CAIS,UAAC,CAAD,EAAO;AACZ,cAAI,KAAI,GAAI,CAAC,CAAC,QAAF,CAAW,IAAvB;AACA,UAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,SAPH;AAQF;AACD;AA3BM;AAlBI,CAAf;;AAiDA,SAAS,aAAT,CAAuB,IAAvB,EAA6B;AAC3B,MACE,uEAAuE,IAAvE,CACE,IADF,CADF,EAIE;AACA,WAAO,IAAP;AACF;;AACA,SAAO,KAAP;AACF","sourcesContent":["<template>\n  <div class=\"main-container\">\n    <h1 class=\"main-title\">Login</h1>\n\n    <div class=\"box-wrap\">\n      <div class=\"\">\n        <div class=\"input-div\">\n          <input\n            type=\"email\"\n            name=\"\"\n            id=\"\"\n            placeholder=\"Email\"\n            v-model=\"email\"\n            class=\"custom-input\"\n          />\n          <input\n            type=\"password\"\n            name=\"\"\n            id=\"\"\n            placeholder=\"Password\"\n            v-model=\"password\"\n            class=\"custom-input\"\n          />\n          <button @click=\"add()\" class=\"theme-btn\">Login</button>\n          <span class=\"error-text\" v-show=\"itemExistMsg !== ''\">{{\n            itemExistMsg\n          }}</span>\n        </div>\n        <p v-if=\"errors.length\">\n    <b>Please correct the following error(s):</b>\n    <ul class=\"error-text\">\n      <li class=\"error\" v-for=\"error in errors\" :key=\"error\">{{ error }}</li>\n    </ul>\n  </p>\n      </div>\n    </div>\n    <div v-show=\"noItemSelectedForMoveMsg !== ''\" class=\"error-div\">\n      {{ noItemSelectedForMoveMsg }}\n    </div>\n  </div>\n</template>\n\n<script>\n// var date = new Date();\n\nif (localStorage.getItem(\"item_management_token\") === null) {\n  // localStorage.setItem(\"item_management_token\", uniqueToken.join(\"\"));\n}\nconst APIURL = \"http://localhost/api/\";\nimport axios from \"axios\";\nimport DefaultLayout from \"../layouts/DefaultLayout\";\nimport { validationMixin } from \"vuelidate\";\n\nexport default {\n  name: \"HelloWorld\",\n  mixins: [validationMixin],\n  created() {\n    this.$emit(\"update:layout\", DefaultLayout);\n  },\n  data() {\n    return {\n      password: \"\",\n      email: \"\",\n      errors: [],\n      right_items: [],\n      item_list: [],\n      itemExistMsg: \"\",\n      active_el: \"\",\n      noItemSelectedForMoveMsg: \"\",\n    };\n  },\n  methods: {\n    add() {\n      this.errors = [];\n      if (!this.email) {\n        this.errors.push(\"Email required.\");\n      } else if (!ValidateEmail(this.email)) {\n        this.errors.push(\"Email format not correct.\");\n      }\n      if (!this.password) {\n        this.errors.push(\"Password required.\");\n      }\n\n      if (this.errors.length == 0) {\n        const data = {\n          email: this.email,\n          password: this.password,\n        };\n        const response = axios.post(APIURL + \"login\", data);\n        response\n          .then((response) => {\n            console.log(response.data);\n          })\n          .catch((e) => {\n            var error = e.response.data;\n            console.log(error);\n          });\n      }\n    },\n  },\n};\n\nfunction ValidateEmail(mail) {\n  if (\n    /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\\.[a-zA-Z0-9-]+)*$/.test(\n      mail\n    )\n  ) {\n    return true;\n  }\n  return false;\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n</style>"],"sourceRoot":""}]}